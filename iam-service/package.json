    "react-codemirror": "^1.0.0",
    "react-image-lightbox": "^4.6.0",
    "react-quill": "^1.2.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint-staged:es"
    ],
