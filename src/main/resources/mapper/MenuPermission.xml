<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.choerodon.iam.infra.mapper.MenuPermissionMapper">
    <delete id="deleteDirtyData">
        delete imp
        from iam_menu_permission imp
        where imp.menu_code
        not in (
          select id from iam_menu_b
        )
    </delete>
</mapper>
